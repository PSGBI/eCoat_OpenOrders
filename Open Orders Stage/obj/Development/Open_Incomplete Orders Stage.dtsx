                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
	varchar(wlmcu,50,37) as WorkCenter,
		wadoco AS OrderNumber, 
		waitm AS ShortItem,
		varchar(walitm,50,37) as PartNumber,
		wadl01 AS PartDesc, 
		varchar(wasrst,50,37) as StatusCode,
		wldrqj AS OpsDateInt, 
		wluorg AS RequestedQTY,
		wlsoqs AS CompletedQTY,
		mcdl01 AS WCDesc,
		varchar(wluom,50,37) UoM,
		varchar(wawr02,50,37) ColorCode,
		DRDL01 AS Color
from amsdta.f4801
		inner join amsdta.f3112 on wadoco=wldoco and wadcto=wldcto
		join amsdta.F0006 on WLMCU = MCMCU
		left outer join amscom.f0005 on DRSY='00' and DRRT='W2' and TRIM(DRKY)=TRIM(WAWR02)
		where wasrst in ('41','42','45') 
		and wamcu = '         100'
		and wlopst &lt;&gt; '90'
		and wluorg &gt; wlsoqs</property>
